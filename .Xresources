! Base16 Flat
! Scheme: Chris Kempson (http://chriskempson.com)

#define base00 #2C3E50 
#define base01 #34495E
#define base02 #7F8C8D
#define base03 #95A5A6
#define base04 #BDC3C7
#define base05 #e0e0e0
#define base06 #f5f5f5
#define base07 #ECF0F1      
#define base08 #E74C3C
#define base09 #E67E22
#define base0A #F1C40F
#define base0B #2ECC71
#define base0C #1ABC9C
#define base0D #3498DB
#define base0E #9B59B6
#define base0F #be643c

*.foreground:   base05
*.background:   base00
*.cursorColor:  base05

*.color0:       base00
*.color1:       base08
*.color2:       base0B
*.color3:       base0A
*.color4:       base0D
*.color5:       base0E
*.color6:       base0C
*.color7:       base05

*.color8:       base03
*.color9:       base08
*.color10:      base0B
*.color11:      base0A
*.color12:      base0D
*.color13:      base0E
*.color14:      base0C
*.color15:      base07

! Note: colors beyond 15 might not be loaded (e.g., xterm, urxvt),
! use 'shell' template to set these if necessary
*.color16:      base09
*.color17:      base0F
*.color18:      base01
*.color19:      base02
*.color20:      base04
*.color21:      base06


!! Xft
Xft.dpi: 96
Xft.antialias:                              true
Xft.hinting:                                true
Xft.rgba:                                   rgb
Xft.autohint:                               false
Xft.hintstyle:                              hintslight
Xft.lcdfilter:                              lcddefault


!! Xcursor
Xcursor.theme:                              Neutral
Xcursor.size:                               0


!! Xterm
XTerm*background:                           #232C31
XTerm*foreground:                           #C5C8C6
XTerm*reverseVideo:                         on
XTerm*faceName: Terminus:                   size=11:antialias=true
XTerm*selectToClipboard:                    true


!! URxvt

URxvt.font:                                 xft:Hack-Regular:pixelsize=24
URxvt.boldFont:                             xft:Hack-Bold:pixelsize=24bold
URxvt.italicFont:                           xft:Hack-Italic=24italic
URxvt.boldItalicFont:                       xft:Hack-BoldItalic=24bold:italic
URxvt.letterSpace:                          -1

URxvt.internalBorder:                       0
URxvt.externalBorder:                       0
URxvt.scrollBar:                            false

! URxvt*fading:                               15
! value for compton???
! URxvt.background:                        [80]baseBG 
! URxvt*fadeColor:                         baseBG
URxvt*saveLines:                           5000

! Separat background color. Original background color (base00) used to match 
! 1st tab of cobalt2 theme to the window-title-bar of my theme
! Remember URxvt. / URxvt no dot at color0!
URxvt*color0:        #2F3540
URxvt*.background:    #193549
!URxvt*foreground:    color          

URxvt*cursorBlink:                          true
!URxvt*.troughColor:                        base00
URxvt*.highlightColor:                      base0D 
URxvt*.highlightTextColor:                  base00
URxvt*.cursorColor:                         base0A
URxvt*.pointerColorBackground:              base06 
URxvt*.pointerColorForeground:              base0A
! URxvt*underlineColor:                     color
! URxvt*scrollColor:                        color
! URxvt*troughColor:                        color
! URxvt*cursorColor2:                       color
! URxvt*pointerColor:                       color
! URxvt*pointerColor2:                      color
! URxvt*borderColor:                        color


!! Rofi
rofi.color-enabled:                  true
! Color scheme for normal row
rofi.color-normal:                   #F5F6F7,#5c616c,#fbfbfb,#5294e2,#ffffff
! Color scheme for urgent row
rofi.color-urgent:                   #F5F6F7,#F04A50,#fbfbfb,#5294e2,#dc322f
! Color scheme for active row
rofi.color-active:                   #F5F6F7,#c2cad0,#fbfbfb,#5294e2,#c2cad0
! Color scheme window
rofi.color-window:                   #F5F6F7,#5c616c
! Separator style (none, dash, solid)
rofi.separator-style:                dash


